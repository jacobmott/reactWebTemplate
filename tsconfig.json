{
    "compilerOptions": {
      "jsx": "react",
      "lib": ["es6","dom"],
      "target": "esnext",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "noEmit": true,
      "skipLibCheck": true,
      "types": ["react/next", "react-dom/next"]
    },
    "exclude": ["./node_modules"]
}